{
    "include": [ "src" ],
    "compilerOptions": {
        "jsx": "react",
        "target": "ES6",
        "module": "commonjs",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
}

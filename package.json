{
	"name": "atelier-core",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"kill": "fuser -k 5000/tcp",
		"start-db": "sudo service mongod start",
		"dev-server": "sudo service mongod start && ts-node-dev --respawn --transpileOnly ./api/src/app.ts ",
		"prod-server": "node ./build/api/src/app.js",
		"start": "node ./build/api/src/app.js",
		"backend": "npm run compile && npm run start",
		"dev-watch": "npx webpack --watch ---config webpack.development.config.js ./client/src",
		"prod-build": "npx webpack ---config webpack.production.config.js ./client/src",
		"test": "mocha test/**/*.spec.ts --require ts-node/register --recursive --exit",
		"test-front": "mocha client/tests/**/*.test.tsx --require ts-node/register --recursive --exit",
		"prepare": "npm run compile",
		"copy-keys": "cp  -a api/keys/ build/api/keys/",
		"style": "npx gts init",
		"clean": "gts clean",
		"compile": "tsc -p .",
		"fix": "gts fix",
		"check": "gts check",
		"database-rebuild": "node build/api/src/database/structure/DatabaseStructure",
		"database-samples": "node build/api/src/database/structure/DatabaseSamples",
		"database-reset": "node build/api/src/database/structure/DatabaseStructure -i"
	},
